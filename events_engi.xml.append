			<text load="HYPERSPACE_TRAVEL_EMERGENCY_TEXT"/>
			<modifyPursuit amount="2"/>
			<choice hidden="true">
				<text>Continue...</text>
				<event load="START_BEACON_QUARANTINE_ACTUAL"/>
			</choice>
		</event>
	</choice>
